datasource db {
  provider = "postgresql"
  url      = env("postgresql://user:password@localhost:5432/mydb?schema=public")
}

generator client {
  provider = "prisma-client-js"
}

model History {
  id         Int      @id @default(autoincrement())
  productId  Int
  shopId     Int
  action     String
  quantity   Int
  createdAt  DateTime @default(now())
}
