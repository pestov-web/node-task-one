generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model History {
  id          Int      @id @default(autoincrement())
  productId   Int
  shopId      Int
  action      String
  quantity    Int
  createdAt   DateTime @default(now())

  @@index([productId])
  @@index([shopId])
  @@index([createdAt])
}
