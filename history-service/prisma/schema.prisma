generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model History {
  id          Int      @id @default(autoincrement())
  productId   Int
  shopId      Int
  action      String
  quantity    Int
  createdAt   DateTime @default(now())

  @@index([productId])
  @@index([shopId])
  @@index([createdAt])
}
